<?xml version="1.0" encoding="utf-8"?>

<FrameLayout android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android">
           <WebView 
        		android:id="@+id/webview0"
        			android:layout_width="fill_parent"
        			android:layout_height="fill_parent"
        			android:clickable="true"
    				/>  

    
    <SlidingDrawer
        android:id="@+id/Drawer"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:isScrollContainer="true"
        
        android:handle="@+id/handle"
        android:content="@+id/GridItem">
             
        <Button
            android:id="@+id/handle"
            android:background="@drawable/drawer_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
            
		<LinearLayout 
   			android:id="@+id/GridItem"
   			android:layout_width="wrap_content"
   			android:layout_height="wrap_content"
   			android:orientation="vertical"
   			android:gravity="center_horizontal"
   			android:background="#000000">
			
				<TableLayout
    			android:layout_width="fill_parent"
    			android:layout_height="fill_parent"
    			android:stretchColumns="*"
    			>
    			<!-- used fill parent on all your widths for square buttons still messy -->
    				<TableRow 
        			android:layout_weight="1"
			        android:gravity="center">
   						<Button android:textSize="10dp"	android:id="@+id/button1" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
						<Button android:textSize="10dp"	android:id="@+id/button2" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
						<Button android:textSize="10dp"	android:id="@+id/button3" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
           			</TableRow>
           			
    				<TableRow 
        			android:layout_weight="1"
        			android:gravity="center">
        				<Button android:textSize="10dp"	android:id="@+id/button4" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        				<Button android:textSize="10dp"	android:id="@+id/button5" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        				<Button android:textSize="10dp" android:id="@+id/button6" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
					</TableRow>
					
    				<TableRow 
        			android:layout_weight="1"
        			android:gravity="center">    	      
        	      		<Button android:textSize="10dp"	android:id="@+id/button7" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        		       	<Button android:textSize="10dp"	android:id="@+id/button8" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        	        	<Button android:textSize="10dp"	android:id="@+id/button9" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
   				    </TableRow>
   				    
   				   	<TableRow 
        			android:layout_weight="1"
        			android:gravity="center">
        				<Button android:textSize="10dp"	android:id="@+id/button10" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        				<Button android:textSize="10dp"	android:id="@+id/button11" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        				<Button android:textSize="10dp"	android:id="@+id/button12" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
					</TableRow>
   				    
   				    <TableRow 
        			android:layout_weight="1"
        			android:gravity="center">
        				<Button android:textSize="10dp"	android:id="@+id/button13" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        				<Button android:textSize="10dp"	android:id="@+id/button14" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
        				<Button android:textSize="10dp"	android:id="@+id/button15" android:layout_width="fill_parent" android:layout_height="fill_parent"></Button>
					</TableRow>
   				    

			</TableLayout>  

	</LinearLayout>

</SlidingDrawer>


   
    
</FrameLayout><!--Hey jeff figured out comments-->

